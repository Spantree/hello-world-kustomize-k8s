datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["selectRelationCount", "nApi", "referentialActions"]
}

// models examples
model Counter {
	id String @id
	name String @unique
	value BigInt
	createdAt DateTime @default(now())
	updatedAt DateTime @updatedAt
}
